<servlet-class>servlets.MainServlets</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Main</servlet-name>
<url-pattern>/main</url-pattern>
</servlet-mapping>
<servlet>
<description></description>
<display-name>FilterLogout</display-name>
<servlet-name>FilterLogout</servlet-name>
<servlet-class>servlets.FilterLogoutServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>FilterLogout</servlet-name>
<url-pattern>/filterlogout</url-pattern>
</servlet-mapping>
<filter>
<filter-name>f1</filter-name>
<filter-class>servlets.FilterServlet</filter-class>
</filter>
<filter-mapping>
<filter-name>f1</filter-name>
<url-pattern>/filterlogin</url-pattern>
</filter-mapping>
</web-app>